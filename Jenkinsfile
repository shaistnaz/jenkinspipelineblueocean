pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'i want to build'
      }
    }

    stage('test') {
      steps {
        echo 'i ant to test'
      }
    }

    stage('stage') {
      steps {
        echo 'i an to stge'
      }
    }

    stage('deploy') {
      steps {
        echo 'i want to deploy'
      }
    }

    stage('operate') {
      steps {
        echo 'i ant to operate'
      }
    }

  }
}